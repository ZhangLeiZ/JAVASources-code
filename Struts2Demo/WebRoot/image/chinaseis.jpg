						sli1 = "_czc.push(['_trackEvent','"+da[h].adNumber+"','点击','"+da[h].adId+"','','']);cli.click_track('"+da[h].adNumber+"','"+da[h].adId+"','');";
						}else{
						sli = "cli.click_track('"+da[h].adNumber+"','"+da[h].adId+"','');";
				sli1 = "cli.click_track('"+da[h].adNumber+"','"+da[h].adId+"','');";
				}
				var slide = "";
				if(i==0){
					slide = slide
										+ " <li style='display: list-item; background: url("
										+ da[h].bigimgUrl
										+ ") 50% 0% no-repeat;'><a href="
										+ da[h].url + " target='_blank' onclick="+sli+"></a></li>";
				}else{
					slide = slide
										+ " <li style='display: none; background: url("
										+ da[h].bigimgUrl
										+ ") 50% 0% no-repeat;'><a href="
										+ da[h].url + " target='_blank' onclick="+sli+"></a></li>";
				}
				$(".slide li:eq("+i+")").replaceWith(slide);
				$(".small-box ul li:eq("+i+") a").attr("href",da[h].url);
				$(".small-box ul li:eq("+i+") a").attr("target","_blank");
				$(".small-box ul li:eq("+i+") a").attr("onclick",sli1);
				$(".small-box ul li:eq("+i+") a img").attr("src",da[h].smallimgUrl);
			}
		}}
	}	
	function getCode(){
		var msg = cdp.getContent();
		if (parseInt(msg.success) == 1) {
			var da = msg.datas;
			for ( var h = 0; h < da.length; h++) {
			if(("insertCode" in da[h])&&da[h].insertCode!=null&&da[h].insertCode.indexOf("<script")>=0){
					var str=da[h].insertCode;
					str=str.substring(str.indexOf("http"),str.indexOf("\"",str.indexOf("http")));
					loadJs(str, function(){});
				}else if(("insertCode" in da[h])&&da[h].insertCode!=null&&da[h].insertCode.indexOf("http://")>=0){
					$("#xcode img:eq("+h+")").attr("src",da[h].insertCode);
				}
			}
			
			}
	}
	window.onload = function() {
		getad();
		getCode();
	}
